   "source": [
    "import torch\n",
    "\n",
    "x = torch.FloatTensor([3])\n",
    "x.requires_grad=False\n",
    "w = torch.FloatTensor([1])\n",
    "w.requires_grad=True\n",
    "b = torch.FloatTensor([1])\n",
    "b.requires_grad=True"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "y = w*x + b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "w grad: tensor([3.])\n",
      "b grad: tensor([1.])\n"
     ]
    }
   ],
   "source": [
    "y.backward()\n",
    "print(\"w grad: \" + str(w.grad))\n",
    "print(\"b grad: \" + str(b.grad))"
   ]
  },
